<main class="main">
  <div class="inner">
      <div class="greeting">
        <% if user_signed_in? %>
          <%= link_to "こんにちは#{current_user.name}さん",  "/users/#{current_user.id}" %>
        <% else %>
        <% end %>
        <%= link_to "", root_path, class: :greeting__link %>
      </div>
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototype %>
    </div>
  </div>
</main>
